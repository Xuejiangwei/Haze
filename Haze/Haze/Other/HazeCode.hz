类 点
{
	数据
	{	
		私
		{
			小数 横坐标
		}
		 
		公
		{
			小数 纵坐标
		}
	}

	函数
	{
		公
		{
			点(小数 甲, 小数 乙)
			{
				横坐标 = 甲
				纵坐标 = 乙
			}

			空 加(点 值)
			{
				横坐标 = 值@横坐标
				纵坐标 = 值@纵坐标
			}

			线 连线(点 甲, 点 乙)
			{
				返回 线(甲, 乙)
			}

			向量 获得向量(点 甲, 点 乙)
			{
				返回 向量(甲, 乙)
			}	
		}
	}
}

主函数()
{
	点 甲()
}