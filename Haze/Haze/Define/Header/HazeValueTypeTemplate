
HAZE_TYPE_DEFINE(None)

HAZE_TYPE_DEFINE(Void)

// 基本类型 Bool -> Float64
HAZE_TYPE_DEFINE(__BaseType_Begin)

HAZE_TYPE_DEFINE(Bool)

HAZE_TYPE_DEFINE(Int8)
HAZE_TYPE_DEFINE(Int16)
HAZE_TYPE_DEFINE(Int32)
HAZE_TYPE_DEFINE(Int64)

HAZE_TYPE_DEFINE(UInt8)
HAZE_TYPE_DEFINE(UInt16)
HAZE_TYPE_DEFINE(UInt32)
HAZE_TYPE_DEFINE(UInt64)

HAZE_TYPE_DEFINE(Float32)
HAZE_TYPE_DEFINE(Float64)

HAZE_TYPE_DEFINE(__BaseType_End)

// 纯字符串, 用作操作数的Variable名字当作dynamicClass的成员名, 虚函数名的字符串
HAZE_TYPE_DEFINE(PureString)

// 引用, 只作用于基本类型, 因为类和数组本来就是指针
HAZE_TYPE_DEFINE(Refrence)

// 地址
HAZE_TYPE_DEFINE(Address)

// 函数指针, 不参与GC
HAZE_TYPE_DEFINE(Function)

// Object函数, Advance类型的函数
HAZE_TYPE_DEFINE(ObjectFunction)

HAZE_TYPE_DEFINE(__Advance_Begin)

// 数组类型 参与GC
HAZE_TYPE_DEFINE(Array)

// 字符串, 不定长, 参与GC
HAZE_TYPE_DEFINE(String)

// 类对象, 参与GC
HAZE_TYPE_DEFINE(Class)

// 动态类, 参与GC
HAZE_TYPE_DEFINE(DynamicClass)

// 基本类型的对象, 参与GC
HAZE_TYPE_DEFINE(ObjectBase)

// 哈希对象, 参与GC
HAZE_TYPE_DEFINE(Hash)

// 闭包, 匿名函数, 参与GC
HAZE_TYPE_DEFINE(Closure)

HAZE_TYPE_DEFINE(__Advance_End)

// 不起作用, 只用来解析时做相同类型判断
HAZE_TYPE_DEFINE(Enum)

// 只能用于库函数的定义中
HAZE_TYPE_DEFINE(MultiVariable)

// 动态类成员类型
HAZE_TYPE_DEFINE(DynamicClassUnknow)