引 标准流

类 父类
{
	数据
	{	
		公
		{
			整数 年龄
		}
	}

	函数
	{
		公
		{
			空 打印名称()
			{
				打印("我是父类 年龄 %d\n", 年龄)
			}
		}
	}
}

类 子类 : 父类
{
	数据
	{	
		公
		{
			整数 性别
		}
	}

	函数
	{
		公
		{
			空 打印名称()
			{
				打印("我是子类 性别 %d\n", 性别)
			}
		}
	}
}

主函数()
{
	子类 甲()
	整数 乙 = 120
	甲之性别 = 10
	甲之打印名称()
	甲之性别 = 乙

	甲之打印名称()
}