类 怪物
{
    数据
    {
        整数 血量
        整数 攻击力
    }

    函数
    {
        公
        {
            虚 空 初始化(整数 血量, 整数 攻击力)
            {
                血量 = 血量
                攻击力 = 攻击力
            }

            空 打印数据()
            {
                打印("怪物 血量 %d 攻击力 %d\n", 血量, 攻击力)
            }
        }
    }
}